{
    "name": "ionic-feedback-sample",
    "version": "0.0.0-development",
    "description": "Sample for ionic-feedback-service and ionic-feedback-viewer",
    "author": {
        "name": "Markus Wagner",
        "email": "markus.wagner02@web.de"
    },
    "repository": "https://github.com/Ritzlgrmft/ionic-feedback-sample.git",
    "license": "MIT",
    "private": false,
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "lint": "tslint --project tsconfig.json --format verbose --type-check",
        "test": "npm run lint && npm run ionic:build && npm run ionic:build --prod",
        "semantic-release": "semantic-release pre && semantic-release post"
    },
    "config": {
        "ionic_copy": "./config/copy.config.js",
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@angular/animations": "4.1.0",
        "@angular/common": "4.1.0",
