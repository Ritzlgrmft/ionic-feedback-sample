        "@angular/compiler-cli": "4.1.0",
        "@angular/core": "4.1.0",
        "@angular/forms": "4.1.0",
        "@angular/http": "4.1.0",
        "@angular/platform-browser": "4.1.0",
        "@angular/platform-browser-dynamic": "4.1.0",
        "@angular/platform-server": "4.1.0",
        "@ionic-native/app-version": "3.10.2",
        "@ionic-native/core": "3.10.2",
        "@ionic-native/device": "3.10.2",
        "@ionic-native/screenshot": "3.10.2",
        "@ionic-native/shake": "3.10.2",
        "@ionic-native/splash-screen": "3.10.2",
        "cordova-ios": "^4.4.0",
        "cz-conventional-changelog": "2.0.0",
        "ionic-angular": "3.2.1",
        "ionic-configuration-service": "5.0.0",
        "ionic-logging-service": "3.1.0",
        "ionicons": "3.0.0",
        "moment": "2.18.1",
        "rxjs": "5.1.1",
        "sw-toolbox": "3.6.0",
        "zone.js": "0.8.10",
        "com.darktalker.cordova.screenshot": "https://github.com/gitawego/cordova-screenshot.git",
        "cordova-plugin-app-version": "0.1.9",
        "cordova-plugin-console": "1.0.5",
        "cordova-plugin-device": "1.1.4",
        "cordova-plugin-splashscreen": "4.0.2"
    },
    "devDependencies": {
        "@ionic/app-scripts": "1.3.7",
        "@ionic/cli-plugin-ionic-angular": "1.1.2",
        "@ionic/cli-plugin-cordova": "1.1.2",
        "@types/cordova": "0.0.34",
        "codelyzer": "3.0.1",
        "tslint": "5.2.0",
        "typescript": "2.2.1",
        "semantic-release": "6.3.6"
    },
    "cordova": {
        "plugins": {
            "com.darktalker.cordova.screenshot": {},
            "cordova-plugin-app-version": {},
            "cordova-plugin-console": {},
            "cordova-plugin-device": {},
            "cordova-plugin-shake": {},
            "cordova-plugin-splashscreen": {}
        },
        "platforms": [
            "ios"
        ]
    }
}